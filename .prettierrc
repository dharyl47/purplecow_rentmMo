{
    "semi": true,                 
    "singleQuote": false,        
    "tabWidth": 2,                
    "useTabs": false,              
    "trailingComma": "none",       
    "bracketSpacing": true,       
    "arrowParens": "avoid",       
    "printWidth": 80,             
    "jsxSingleQuote": false,       
    "jsxBracketSameLine": false,  
    "endOfLine": "auto",         
    "quoteProps": "as-needed",     
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json"
        }
      }
    ]
  }
  